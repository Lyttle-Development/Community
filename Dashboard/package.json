{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "start": "systemctl show-environment > .env && next start -p 3000",
    "dev:start": "npx rimraf ./.next && next dev -p 8080",
    "deploy": "npm run build",
    "docker:setup": "npm run build",
    "docker:service:main": "cp services/main/application.docker.community-v3.dashboard.service /etc/systemd/system/application.docker.community-v3.dashboard.service && nano /etc/systemd/system/application.docker.community-v3.dashboard.service",
    "docker:service:beta": "cp services/beta/application.docker.community-v3.dashboard.service /etc/systemd/system/application.docker.community-v3.dashboard.service && nano /etc/systemd/system/application.docker.community-v3.dashboard.service",
    "docker:service:alpha": "cp services/alpha/application.docker.community-v3.dashboard.service /etc/systemd/system/application.docker.community-v3.dashboard.service && nano /etc/systemd/system/application.docker.community-v3.dashboard.service",
    "docker:service:dev": "cp services/dev/application.docker.community-v3.dashboard.service /etc/systemd/system/application.docker.community-v3.dashboard.service && nano /etc/systemd/system/application.docker.community-v3.dashboard.service"
  },
  "dependencies": {
    "@apollo/client": "^3.9.6",
    "@types/node": "20.11.25",
    "@types/react": "18.2.64",
    "@types/react-dom": "18.2.21",
    "diff": "^5.2.0",
    "discord.js": "^14.14.1",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "gsap": "^3.12.5",
    "next": "^14.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.1",
    "react-transition-group": "^4.4.5",
    "remark-gfm": "^4.0.0",
    "sass": "^1.71.1",
    "typescript": "5.4.2"
  },
  "devDependencies": {
    "@types/diff": "^5.0.9",
    "@types/react-transition-group": "^4.4.10"
  }
}
